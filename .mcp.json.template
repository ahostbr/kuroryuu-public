{
  "mcpServers": {
    "kuroryuu": {
      "type": "http",
      "url": "http://127.0.0.1:8100/mcp"
    },
    "KURORYUU_STDIO_MCP": {
      "command": "{{KURORYUU_VENV}}\\Scripts\\python.exe",
      "args": ["{{KURORYUU_ROOT}}\\apps\\mcp_stdio\\server.py"],
      "env": {
        "KURORYUU_RAG_USE_RG": "1",
        "KURORYUU_PROJECT_ROOT": "{{KURORYUU_ROOT}}"
      },
      "disabled": true
    }
  }
}
